<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextArea?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionary.controller.GameController">
   <children>
      <VBox>
         <HBox fillHeight="false">
            <children>
               <Button fx:id="homeNav" focusTraversable="false" mnemonicParsing="false" text="HOME">
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
               <Button fx:id="gameNav" focusTraversable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="GAME">
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
               <Button fx:id="myListNav" focusTraversable="false" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" text="MY LIST">
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
            </children>
         </HBox>
         <TextArea prefHeight="319.0" prefWidth="600.0" text="THIS IS MY GAME" />
      </VBox>
   </children>
</AnchorPane>
