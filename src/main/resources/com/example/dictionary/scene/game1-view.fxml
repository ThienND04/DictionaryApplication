<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="400.0" prefWidth="869.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionary.controller.Game1Controller">
    <children>
        <VBox maxHeight="1000.0" maxWidth="1000.0" style="-fx-background-color: gray;">
            <HBox fillHeight="false">
                <children>
                    <Button fx:id="homeNav" focusTraversable="false" mnemonicParsing="false" text="HOME">
                        <cursor>
                            <Cursor fx:constant="HAND" />
                        </cursor>
                    </Button>
                    <Button fx:id="gameNav" focusTraversable="false" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="GAME">
                        <cursor>
                            <Cursor fx:constant="HAND" />
                        </cursor>
                    </Button>
                    <Button fx:id="myListNav" focusTraversable="false" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" text="MY LIST">
                        <cursor>
                            <Cursor fx:constant="HAND" />
                        </cursor>
                    </Button>
                </children>
            </HBox>
         <Label prefHeight="45.0" prefWidth="98.0" text="GAME 1" />
        </VBox>
      <WebView fx:id="question" layoutX="71.0" layoutY="97.0" prefHeight="69.0" prefWidth="442.0" />
      <Button fx:id="ans1" layoutX="14.0" layoutY="206.0" mnemonicParsing="false" onAction="#ans1BtnClicked" prefHeight="121.0" prefWidth="174.0" style="-fx-background-color: steelblue;" wrapText="true" />
      <Button fx:id="ans2" layoutX="213.0" layoutY="206.0" mnemonicParsing="false" onAction="#ans2BtnClicked" prefHeight="121.0" prefWidth="174.0" style="-fx-background-color: steelblue;" wrapText="true" />
      <Button fx:id="ans3" layoutX="412.0" layoutY="206.0" mnemonicParsing="false" onAction="#ans3BtnClicked" prefHeight="121.0" prefWidth="174.0" style="-fx-background-color: steelblue;" wrapText="true" />
      <ButtonBar layoutX="302.0" layoutY="344.0" prefHeight="40.0" prefWidth="306.0">
         <buttons>
            <Button fx:id="prevBtn" mnemonicParsing="false" onAction="#prevBtnClicked" text="Previous" />
            <Button fx:id="nextPrev" mnemonicParsing="false" onAction="#nextBtnClicked" text="Next" />
            <Button fx:id="submitBtn" minWidth="57.0" mnemonicParsing="false" onAction="#submitBtnClicked" prefHeight="24.0" text="Submit" />
         </buttons>
      </ButtonBar>
    </children>
</AnchorPane>
